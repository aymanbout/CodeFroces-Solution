line=input("")
line=line.lower()
x=len(line)
for i in range(0,x):
	if line[i] == "a" :
		print("",end="")
	elif line[i] == "u":
		print("",end="")
	elif line[i] == "y":
		print("",end="")
	elif line[i] == "e":
		print("",end="")
	elif line[i] == "i":
		print("",end="")
	elif line[i] == "o":
		print("",end="")
	else: 
		print(".",end="")
		print(line[i],end="")

