line=input("")
x=len(line)
for i in range(0,x-1):
	if line[i] == "a":
		line = line.replace(line[i], "")
	elif line[i] == "u":
		line = line.replace(line[i], "")
	elif line[i] == "y":
		line = line.replace(line[i], "")
	elif line[i] == "e":
		line = line.replace(line[i], "")
	elif line[i] == "i":
		line = line.replace(line[i], "")
	elif line[i] == "o":
		line = line.replace(line[i], "")
	else: 
		print(".",end="")
		print(line[i].lower(),end="")


